mponents/Block.js","original":{"line":38,"column":4}},{"generated":{"line":100,"column":0},"source":"components/Block.js","original":{"line":46,"column":3}},{"name":"Component","generated":{"line":104,"column":2},"source":"components/Block.js","original":{"line":5,"column":35}}],"sources":{"components/Block.js":"import React, { Component } from \"react\";\nimport { Button } from \"react-bootstrap\";\nimport Transaction from \"./Transaction\";\n\nexport default class Block extends Component {\n  state = { displayTransaction: false };\n\n  toggleTransaction = () => {\n    this.setState({ displayTransaction: !this.state.displayTransaction });\n  };\n\n  get displayTransaction() {\n    const { data } = this.props.block;\n    const stringfiedData = JSON.stringify(data);\n\n    const dataDisplay =\n      stringfiedData.length > 35\n        ? `${stringfiedData.substring(0, 35)}...`\n        : stringfiedData;\n\n    if (this.state.displayTransaction) {\n      return (\n        <div>\n          {data.map(transaction => (\n            <div key={transaction}>\n              <hr />\n              <Transaction transaction={transaction} />\n            </div>\n          ))}\n          <br />\n          <Button variant=\"danger\" size=\"sm\" onClick={this.toggleTransaction}>\n            Show Less\n          </Button>\n        </div>\n      );\n    }\n\n    return (\n      <div>\n        <div>Data: {dataDisplay}</div>\n        <Button variant=\"danger\" size=\"sm\" onClick={this.toggleTransaction}>\n          Show More\n        </Button>\n      </div>\n    );\n  }\n  render() {\n    const { timestamp, hash } = this.props.block;\n\n    const hashDisplay = `${hash.substring(0, 15)}...`;\n\n    return (\n      <div className=\"Block\">\n        <div>Hash: {hashDisplay}</div>\n        <div>Timestamp: {new Date(timestamp).toLocaleDateString()}</div>\n        {this.displayTransaction}\n      </div>\n    );\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"f17eac016d1e6b46832ab9db51117